msgctxt "secretf/09_town3"
msgid ""
"\n"
"Hi GeoBunny :)"
msgstr ""
"\n"
"ПРИВЕТ ГЕОКРОЛИК :)"

#: .fake/Translations/secretf/09_town3.j2l.h:6
msgctxt "secretf/09_town3"
msgid ""
"\n"
"Goto www.project2.com\n"
